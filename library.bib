@article{loubetAcceleratingAllatomMD2014,
  title = {Accelerating All-Atom {{MD}} Simulations of Lipids Using a Modified Virtual-Sites Technique},
  author = {Loubet, Bastien and Kopec, Wojciech and Khandelia, Himanshu},
  year = {2014},
  month = dec,
  journal = {Journal of Chemical Theory and Computation},
  volume = {10},
  number = {12},
  pages = {5690--5695},
  publisher = {{American Chemical Society}},
  doi = {10.1021/ct500100f},
  abstract = {We present two new implementations of the virtual sites technique which completely suppresses the degrees of freedom of the hydrogen atoms in a lipid bilayer allowing for an increased time step of 5 fs in all-atom simulations of the CHARMM36 force field. One of our approaches uses the derivation of the virtual sites used in GROMACS while the other uses a new definition of the virtual sites of the CH2 groups. Our methods is tested on a DPPC (no unsaturated chain), a POPC (one unsaturated chain), and a DOPC (two unsaturated chains) lipid bilayers. We calculate various physical properties of the membrane of our simulations with and without virtual sites and explain the differences and similarity observed. The best agreements are obtained for the GROMACS original virtual sites on the DOPC bilayer where we get an area per lipid of 67.3 {$\pm$} 0.3 A\r{}2 without virtual sites and 67.6 {$\pm$} 0.3 A\r{}2 with virtual sites. In conclusion the virtual-sites technique on lipid membranes is a powerful simulation tool, but it should be used with care. The procedure can be applied to other force fields and lipids in a straightforward manner. (Figure Presented).}
}

