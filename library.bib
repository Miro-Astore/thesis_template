  langid = {english},
  pmid = {96110},
  keywords = {Alcaligenes,Bacteria,Clostridium,Escherichia coli,Malate Dehydrogenase,Molecular Weight,Pseudomonas,Rhizobium,Rhodospirillum rubrum,Species Specificity}
