}

@article{hoffmann2018,
  title = {Combining Theoretical and Experimental Data to Decipher {{CFTR 3D}} Structures and Functions},
  author = {Hoffmann, Brice and Elbahnsi, Ahmad and Lehn, Pierre and D{\'e}cout, Jean Luc and Pietrucci, Fabio and Mornon, Jean Paul and Callebaut, Isabelle},
