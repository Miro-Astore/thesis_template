@article{brewington2018,
  title = {Brushed Nasal Epithelial Cells Are a Surrogate for Bronchial Epithelial {{CFTR}} Studies},
  author = {Brewington, John J. and Filbrandt, Erin T. and LaRosa, F. J. and Moncivaiz, Jessica D. and Ostmann, Alicia J. and Strecker, Lauren M. and Clancy, John P.},
