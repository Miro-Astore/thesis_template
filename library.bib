issn = {20734409},
journal = {Cells},
keywords = {amphipol:A8-35,catalytic activity,functional reconstitution,intrinsic anion channel activity,membrane protein purification,proteoliposomal flux},
month = {jul},
number = {8},
pages = {804},
pmid = {31370288},
publisher = {NLM (Medline)},
title = {{Cholesterol Interaction Directly Enhances Intrinsic Activity of the Cystic Fibrosis Transmembrane Conductance Regulator (CFTR)}},
