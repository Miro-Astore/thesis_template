file = {:home/miro/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yue et al. - 2019 - Distinct functions of TMC channels a comparative overview.pdf:pdf},
issn = {14209071},
journal = {Cellular and Molecular Life Sciences},
keywords = {C. elegans,Drosophila,Hearing,Mechanosensation,Membrane excitability,TMC channels,Zebrafish},
month = {oct},
