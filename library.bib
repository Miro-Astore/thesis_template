@article{fay2018,
  title = {Cryo-{{EM Visualization}} of an {{Active High Open Probability CFTR Anion Channel}}},
  author = {Fay, Jonathan F. and Aleksandrov, Luba A. and Jensen, Timothy J. and Cui, Liying L. and Kousouros, Joseph N. and He, Lihua and Aleksandrov, Andrei A. and Gingerich, Drew S. and Riordan, John R. and Chen, James Z.},
  year = {2018},
  month = oct,
  journal = {Biochemistry},
  volume = {57},
  number = {43},
  pages = {6234--6246},
  publisher = {{American Chemical Society}},
  issn = {0006-2960},
  doi = {10.1021/acs.biochem.8b00763},
  abstract = {The cystic fibrosis transmembrane conductance regulator (CFTR) anion channel, crucial to epithelial salt and water homeostasis, and defective due to mutations in its gene in patients with cystic fibrosis, is a unique member of the large family of ATP-binding cassette transport proteins. Regulation of CFTR channel activity is stringently controlled by phosphorylation and nucleotide binding. Structural changes that underlie transitions between active and inactive functional states are not yet fully understood. Indeed the first 3D structures of dephosphorylated, ATP-free, and phosphorylated ATP-bound states were only recently reported. Here we have determined the structure of inactive and active states of a thermally stabilized CFTR, the latter with a very high channel open probability, confirmed after reconstitution into proteoliposomes. These structures, obtained at nominal resolution of 4.3 and 6.6 \AA, reveal a unique repositioning of the transmembrane helices and regulatory domain density that provide insights into the structural transition between active and inactive functional states of CFTR. Moreover, we observe an extracellular vestibule that may provide anion access to the pore due to the conformation of transmembrane helices 7 and 8 that differs from the previous orthologue CFTR structures. In conclusion, our work contributes detailed structural information on an active, open state of the CFTR anion channel.},
  file = {/home/miro/Zotero/storage/2SIE7HKR/acs.biochem.html}
}

