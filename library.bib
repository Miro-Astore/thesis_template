pmid = {18305154},
publisher = {National Academy of Sciences},
title = {{Phenylalanine-508 mediates a cytoplasmic-membrane domain contact in the CFTR 3D structure crucial to assembly and channel function}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18305154 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2265173},
