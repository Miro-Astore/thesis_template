@article{cormet-boyaka2002,
  title = {{{CFTR}} Chloride Channels Are Regulated by a {{SNAP-23}}/Syntaxin {{1A}} Complex},
  author = {{Cormet-Boyaka}, Estelle and Di, Anke and Chang, Steven Y. and Naren, Anjaparavanda P. and Tousson, Albert and Nelson, Deborah J. and Kirk, Kevin L.},
