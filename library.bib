pmid = {28340353},
title = {{Molecular Structure of the Human CFTR Ion Channel}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/28340353 https://linkinghub.elsevier.com/retrieve/pii/S0092867417302398},
volume = {169},
