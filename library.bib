  title = {{{CFTR}}: {{New}} Insights into Structure and Function and Implications for Modulation by Small Molecules},
  author = {Kleizen, Bertrand and Hunt, John F. and Callebaut, Isabelle and Hwang, Tzyh Chang and {Sermet-Gaudelus}, Isabelle and Hafkemeyer, Sylvia and Sheppard, David N.},
