@article{brown2015,
  title = {Neddylation {{Promotes Ubiquitylation}} and {{Release}} of {{Ku}} from {{DNA-Damage Sites}}},
  author = {Brown, Jessica S. and Lukashchuk, Natalia and {Sczaniecka-Clift}, Matylda and Britton, S{\'e}bastien and {le Sage}, Carlos and Calsou, Patrick and Beli, Petra and Galanty, Yaron and Jackson, Stephen P.},
  year = {2015},
  month = may,
  journal = {Cell Reports},
  volume = {11},
  number = {5},
  pages = {704--714},
  publisher = {{Elsevier}},
  issn = {22111247},
  doi = {10.1016/j.celrep.2015.03.058},
  abstract = {The activities of many DNA-repair proteins are controlled through reversible covalent modification by ubiquitin and ubiquitin-like molecules. Nonhomologous end-joining (NHEJ) is the predominant DNAdouble-strand break (DSB) repair pathway in mammalian cells and is initiated by DSB ends being recognized by the Ku70/Ku80 (Ku) heterodimer. By using MLN4924, an anti-cancer drug in clinical trials that specifically inhibits conjugation of the ubiquitin-like protein, NEDD8, to target proteins, we demonstrate that NEDD8 accumulation at DNA-damage sites is a highly dynamic process. In addition, we show that depleting cells of the NEDD8 E2-conjugating enzyme, UBE2M, yields ionizing radiation hypersensitivity and reduced cell survival following NHEJ. Finally, we demonstrate that neddylation promotes Ku ubiquitylation after DNA damage and release of Ku and Ku-associated proteins from damage sites following repair. These studies provide insights into how the NHEJ core complex dissociates from repair sites and highlight its importance for cell survival following DSB induction.},
  pmid = {25921528}
}

@article{brownell2010,
  title = {Substrate-{{Assisted Inhibition}} of {{Ubiquitin-like Protein-Activating Enzymes}}: {{The NEDD8 E1 Inhibitor MLN4924 Forms}} a {{NEDD8-AMP Mimetic In Situ}}},
  author = {Brownell, James E. and Sintchak, Michael D. and Gavin, James M. and Liao, Hua and Bruzzese, Frank J. and Bump, Nancy J. and Soucy, Teresa A. and Milhollen, Michael A. and Yang, Xiaofeng and Burkhardt, Anne L. and Ma, Jingya and Loke, Huay Keng and Lingaraj, Trupti and Wu, Dongyun and Hamman, Kristin B. and Spelman, James J. and Cullis, Courtney A. and Langston, Steven P. and Vyskocil, Stepan and Sells, Todd B. and Mallender, William D. and Visiers, Irache and Li, Ping and Claiborne, Christopher F. and Rolfe, Mark and Bolen, Joseph B. and Dick, Lawrence R.},
