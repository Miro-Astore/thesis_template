pages = {818--824},
pmid = {23442960},
publisher = {The Biophysical Society},
title = {{Specific binding of chloride ions to lipid vesicles and implications at molecular scale}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23442960 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3576540},
