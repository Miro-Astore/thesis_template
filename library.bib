@article{knapp2018,
  title = {Avoiding {{False Positive Conclusions}} in {{Molecular Simulation}}: {{The Importance}} of {{Replicas}}},
  shorttitle = {Avoiding {{False Positive Conclusions}} in {{Molecular Simulation}}},
  author = {Knapp, Bernhard and Ospina, Luis and Deane, Charlotte M.},
  year = {2018},
  month = dec,
  journal = {Journal of Chemical Theory and Computation},
  volume = {14},
  number = {12},
  pages = {6127--6138},
  publisher = {{American Chemical Society}},
  issn = {1549-9618},
  doi = {10.1021/acs.jctc.8b00391},
  abstract = {Molecular simulations are a computational technique used to investigate the dynamics of proteins and other molecules. The free energy landscape of these simulations is often rugged, and minor differences in the initial velocities, floating-point precision, or underlying hardware can cause identical simulations (replicas) to take different paths in the landscape. In this study we investigated the magnitude of these effects based on 310\,000 ns of simulation time. We performed 100 identically parametrized replicas of 3000 ns each for a small 10 amino acid system as well as 100 identically parametrized replicas of 100 ns each for an 827 residue T-cell receptor/MHC system. Comparing randomly chosen subgroups within these replica sets, we estimated the reproducibility and reliability that can be achieved by a given number of replicas at a given simulation time. These results demonstrate that conclusions drawn from single simulations are often not reproducible and that conclusions drawn from multiple shorter replicas are more reliable than those from a single longer simulation. The actual number of replicas needed will always depend on the question asked and the level of reliability sought. On the basis of our data, it appears that a good rule of thumb is to perform a minimum of five to 10 replicas.},
  file = {/home/miro/Zotero/storage/USGWJJQ8/Knapp et al. - 2018 - Avoiding False Positive Conclusions in Molecular S.pdf;/home/miro/Zotero/storage/PBLQMUPA/acs.jctc.html}
}

@article{knecht2013,
  title = {Specific Binding of Chloride Ions to Lipid Vesicles and Implications at Molecular Scale},
  author = {Knecht, Volker and Klasczyk, Benjamin},
  year = {2013},
  month = feb,
  journal = {Biophysical Journal},
  volume = {104},
  number = {4},
  pages = {818--824},
  publisher = {{The Biophysical Society}},
  issn = {00063495},
  doi = {10.1016/j.bpj.2012.12.056},
  abstract = {Biological membranes composed of lipids and proteins are in contact with electrolytes like aqueous NaCl solutions. Based on molecular dynamics studies it is widely believed that Na+ ions specifically bind to 1-palmitoyl-2-oleoyl-sn-glycero-3-phosphocholine (POPC) membranes, whereas Cl- ions stay in solution. Here, we present a careful comparison of recent data from electrophoresis and isothermal titration calorimetry experiments as well as molecular dynamics simulations suggesting that in fact both ions show very similar affinities. The corresponding binding constants are 0.44({$\pm$}0.05) M-1 for Na+ and 0.40({$\pm$}0.04) M-1 for Cl- ions. This is highlighted by our observation that a widely used simulation setup showing asymmetric affinities of Na + and Cl- for POPC bilayers overestimates the effect of NaCl on the electrophoretic mobility of a POPC membrane by an order of magnitude. Implications for previous simulation results on the effect of NaCl on polarization of interfacial water, transmembrane potentials, and mechanisms for ion transport through bilayers are discussed. Our findings suggest that a range of published simulations results on the interaction of NaCl with phosphocholine bilayers have to be reconsidered and revised and that force field refinements are necessary for reliable simulation studies of membranes at physiological conditions on a molecular level. \textcopyright{} 2013 Biophysical Society.},
  pmid = {23442960}
}

