@article{casalino2020,
  title = {{{AI-Driven Multiscale Simulations Illuminate Mechanisms}} of {{SARS-CoV-2 Spike Dynamics}}},
  author = {Casalino, Lorenzo and Dommer, Abigail and Gaieb, Zied and Barros, Emilia P and Sztain, Terra and Ahn, Surl-Hee and Trifan, Anda and Brace, Alexander and Bogetti, Anthony and Ma, Heng and Lee, Hyungro and Turilli, Matteo and Khalid, Syma and Chong, Lillian and Simmerling, Carlos and Hardy, David J and C Maia, Julio D and Phillips, James C and Kurth, Thorsten and Stern, Abraham and Huang, Lei and McCalpin, John and Tatineni, Mahidhar and Gibbs, Tom and Stone, John E and Jha, Shantenu and Ramanathan, Arvind and Amaro, Rommie E},
  year = {2020},
