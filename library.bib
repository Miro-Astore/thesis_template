@article{chang2008,
  title = {Role of {{N-linked}} Oligosaccharides in the Biosynthetic Processing of the Cystic Fibrosis Membrane Conductance Regulator},
  author = {Chang, Xiu-bao and Mengos, April and Hou, Yue-xian and Cui, Liying and Jensen, Timothy J. and Aleksandrov, Andrei and Riordan, John R. and Gentzsch, Martina},
