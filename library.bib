journal = {Nucleic Acids Research},
number = {W1},
pages = {W443--W447},
pmid = {25873628},
publisher = {Oxford University Press},
title = {{PLIP: Fully automated protein-ligand interaction profiler}},
volume = {43},
year = {2015}
}
