file = {:home/miro/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Yamamoto et al. - 2016 - Interactions of Pleckstrin Homology Domains with Membranes Adding Back the Bilayer via High-Throughput Molecula.pdf:pdf},
