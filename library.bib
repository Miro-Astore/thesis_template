@article{flood2019a,
  title = {Atomistic {{Simulations}} of {{Membrane Ion Channel Conduction}}, {{Gating}}, and {{Modulation}}},
  author = {Flood, Emelie and Boiteux, C{\'e}line and Lev, Bogdan and Vorobyov, Igor and Allen, Toby W.},
  year = {2019},
