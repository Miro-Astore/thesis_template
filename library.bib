@article{churchill2019,
  title = {{{EDEn}}\textendash{{Electroceutical Design Environment}}: {{Ion Channel Tissue Expression Database}} with {{Small Molecule Modulators}}},
  shorttitle = {{{EDEn}}\textendash{{Electroceutical Design Environment}}},
  author = {Churchill, Cassandra D. M. and Winter, Philip and Tuszynski, Jack A. and Levin, Michael},
  year = {2019},
