@article{hekmat2007,
  title = {Advanced Protein Crystallization Using Water-Soluble Ionic Liquids as Crystallization Additives},
  author = {Hekmat, Dariusch and Hebel, Dirk and Joswig, Sebastian and Schmidt, Michael and {Weuster-Botz}, Dirk},
  year = {2007},
