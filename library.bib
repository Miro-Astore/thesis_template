@incollection{Dawson2013,
abstract = {This review will focus exclusively on the anion channel function of CFTR (Fig. 1), in particular the anion conduction mechanism. We will attempt to paint a picture of how the channel conducts anions, as inferred from studies of channel function in the wild type and mutant proteins. On the one hand this picture is, of necessity, somewhat fuzzy at this time as the structure of the channel is unknown, even though the structures of homologous, non-channel relatives have been determined,16-19 as has that of a bacterial ClC channel.20,20a On the other hand, the pattern of anion selectivity that has emerged from studies of permeation and channel blockade offers some limited insights into the environment of the pore as experienced by visiting anions, and invites some speculations about the logic of anion channel design. The review will be selective rather than comprehensive and will focus on those studies that speak most directly to elements of pore design.},
author = {Dawson, David C. and Liu, Xuehong and Zhang, Zhi-Ren and McCarty, Nael A.},
booktitle = {The Cystic Fibrosis Transmembrane Conductance Regulator},
