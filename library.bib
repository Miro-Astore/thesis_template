issn = {00219258},
journal = {Journal of Biological Chemistry},
keywords = {Animals,Arginine / chemistry,Asta Jurkuvenaite,Biological,Biotinylation,COS Cells,Cell Membrane / metabolism,Chlorocebus aethiops,Cloning,Cystic Fibrosis Transmembrane Conductance Regulato,Endocytosis,Extramural,Fluorescent Antibody Technique,Fluorescent Dyes / pharmacology,Immunoprecipitation,Indirect,James F Collawn,Karoly Varga,MEDLINE,Models,Molecular,Mutation*,N.I.H.,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,Patch-Clamp Techniques,Phenotype,Polymerase Chain Reaction,Protein Structure,PubMed Abstract,Quinolinium Compounds / pharmacology,Research Support,Tertiary,Transfection,Tyrosine / chemistry,doi:10.1074/jbc.M508131200,pmid:16339147},
