@article{jiang2020,
  title = {Computational {{Dissection}} of {{Membrane Transport}} at a {{Microscopic Level}}},
  author = {Jiang, Tao and Wen, Po Chao and Trebesch, Noah and Zhao, Zhiyu and Pant, Shashank and Kapoor, Karan and Shekhar, Mrinal and Tajkhorshid, Emad},
