@article{thomas2020,
  title = {Structural and Functional Diversity Calls for a New Classification of {{ABC}} Transporters},
  author = {Thomas, Christoph and Aller, Stephen G. and Beis, Konstantinos and Carpenter, Elisabeth P. and Chang, Geoffrey and Chen, Lei and Dassa, Elie and Dean, Michael and Duong Van Hoa, Franck and Ekiert, Damian and Ford, Robert and Gaudet, Rachelle and Gong, Xin and Holland, I. Barry and Huang, Yihua and Kahne, Daniel K. and Kato, Hiroaki and Koronakis, Vassilis and Koth, Christopher M. and Lee, Youngsook and Lewinson, Oded and Lill, Roland and Martinoia, Enrico and Murakami, Satoshi and Pinkett, Heather W. and Poolman, Bert and Rosenbaum, Daniel and Sarkadi, Balazs and Schmitt, Lutz and Schneider, Erwin and Shi, Yigong and Shyng, Show-Ling and Slotboom, Dirk J. and Tajkhorshid, Emad and Tieleman, D. Peter and Ueda, Kazumitsu and V{\'a}radi, Andr{\'a}s and Wen, Po-Chao and Yan, Nieng and Zhang, Peng and Zheng, Hongjin and Zimmer, Jochen and Tamp{\'e}, Robert},
