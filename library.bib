publisher = {Elsevier},
title = {{Neddylation Promotes Ubiquitylation and Release of Ku from DNA-Damage Sites}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25921528 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4431666},
