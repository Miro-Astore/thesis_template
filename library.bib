pages = {5199--5208},
publisher = {American Chemical Society},
title = {{Replica-Exchange Umbrella Sampling Combined with Gaussian Accelerated Molecular Dynamics for Free-Energy Calculation of Biomolecules}},
volume = {15},
year = {2019}
}
