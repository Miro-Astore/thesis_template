@article{bienvenu1996,
  title = {A Novel Missense Mutation in Exon 16 of the Cystic Fibrosis Transmembrane Conductance Regulator ({{CFTR}}) Gene Identified in {{CBAVD}} Patients},
  author = {Bienvenu, Thierry and Hubert, Dominique and Setbon, Eric and Dusser, Daniel and Kaplan, Jean-Claude and Beldjord, Cherif},
  year = {1996},
