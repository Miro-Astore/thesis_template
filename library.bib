journal = {Scientific Reports},
keywords = {Renal cell carcinoma},
month = {dec},
number = {1},
pages = {1--9},
pmid = {28717191},
publisher = {Nature Publishing Group},
title = {{MLN4924 (Pevonedistat), a protein neddylation inhibitor, suppresses proliferation and migration of human clear cell renal cell carcinoma}},
