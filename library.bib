@article{ge2013,
  title = {A Versatile Protocol for the Ionothermal Synthesis of Nanostructured Nickel Compounds as Energy Storage Materials from a Choline Chloride-Based Ionic Liquid},
  author = {Ge, X. and Gu, C. D. and Lu, Y. and Wang, X. L. and Tu, J. P.},
  year = {2013},
  journal = {Journal of Materials Chemistry A},
  volume = {1},
  number = {43},
  pages = {13454--13461},
  issn = {20507496},
  doi = {10.1039/c3ta13303f},
  abstract = {A versatile ionothermal strategy is proposed to synthesize nanostructured nickel compounds as energy storage materials, such as Ni(NH\textbackslash textlessinf\textbackslash textgreater3\textbackslash textless/inf\textbackslash textgreater) \textbackslash textlessinf\textbackslash textgreater6\textbackslash textless/inf\textbackslash textgreaterCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater crystals, nanosheet-like NiCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater, nanoflower-like (NF) {$\alpha$}-Ni(OH)\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater, and mesoporous NF-NiO. The primal solution is a choline chloride (ChCl)-urea mixture-based ionic liquid with nickel chloride hexahydrate (NiCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater{$\cdot$}6H\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreaterO) dissolved. The proposed ionothermal protocol is attractive and environmental friendly because ChCl and urea are naturally biocompatible compounds. It has been reported that Ni(NH\textbackslash textlessinf\textbackslash textgreater3\textbackslash textless/inf\textbackslash textgreater)\textbackslash textlessinf\textbackslash textgreater6\textbackslash textless/inf\textbackslash textgreaterCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater is an indirect and reversible hydrogen storage material for fuel cells. Herein, we demonstrate that Ni(NH\textbackslash textlessinf\textbackslash textgreater3\textbackslash textless/inf\textbackslash textgreater)\textbackslash textlessinf\textbackslash textgreater6\textbackslash textless/inf\textbackslash textgreaterCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater crystals with octahedral geometry and an open structure can be obtained by maintaining the primal solution in a sealed vessel at 150 \textdegree C for 4 hours. A kinetic-to-dynamic growth mechanism is proposed to elucidate the formation of the interesting Ni(NH\textbackslash textlessinf\textbackslash textgreater3\textbackslash textless/inf\textbackslash textgreater)\textbackslash textlessinf\textbackslash textgreater6\textbackslash textless/inf\textbackslash textgreaterCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater structure. Further annealing of the Ni(NH\textbackslash textlessinf\textbackslash textgreater3\textbackslash textless/inf\textbackslash textgreater)\textbackslash textlessinf\textbackslash textgreater6\textbackslash textless/inf\textbackslash textgreaterCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater crystals at 300 \textdegree C produces sheet-like NiCl\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater, which can be utilized as an active material in sodium-ion batteries. Interestingly, with the addition of a small amount of water into the primal solution which has been kept at 150 \textdegree C and open to the air for 40 minutes, NF {$\alpha$}-Ni(OH)\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater can be obtained. Further annealing of the NF {$\alpha$}-Ni(OH)\textbackslash textlessinf\textbackslash textgreater2\textbackslash textless/inf\textbackslash textgreater at 300 \textdegree C leads to the formation of NF-NiO with a mesoporous structure, which is demonstrated to be a promising pseudo-capacitive electrode material in a KOH aqueous solution. The rapid activation process, good cycling performance, and high specific capacitance of the NF-NiO electrode are attributed to its high surface area (72.9 m\textbackslash textlesssup\textbackslash textgreater2\textbackslash textless/sup\textbackslash textgreater g\textbackslash textlesssup\textbackslash textgreater-1\textbackslash textless/sup\textbackslash textgreater) and the mesoporous structure. We expect that the ionothermal method will have implications for the use of ChCl-urea mixture-based ionic liquids in the large-scale synthesis of advanced functional materials. \textcopyright{} The Royal Society of Chemistry 2013.}
}

