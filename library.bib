@article{dekkers2013,
  title = {A Functional {{CFTR}} Assay Using Primary Cystic Fibrosis Intestinal Organoids},
  author = {Dekkers, Johanna F. and Wiegerinck, Caroline L. and {de Jonge}, Hugo R. and Bronsveld, Inez and Janssens, Hettie M. and {de Winter-de Groot}, Karin M. and Brandsma, Arianne M. and {de Jong}, Nienke W. M. and Bijvelds, Marcel J. C. and Scholte, Bob J. and Nieuwenhuis, Edward E. S. and {van den Brink}, Stieneke and Clevers, Hans and {van der Ent}, Cornelis K. and Middendorp, Sabine and Beekman, Jeffrey M.},
  year = {2013},
