file = {:home/miro/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Naughton, Kalli, Sansom - 2016 - Association of Peripheral Membrane Proteins with Membranes Free Energy of Binding of GRP1 PH Domain wit.pdf:pdf},
