@article{hills2016,
  title = {Model Parameters for Simulation of Physiological Lipids},
  author = {Hills, Ronald D. and McGlinchey, Nicholas},
  year = {2016},
  month = may,
  journal = {Journal of Computational Chemistry},
  volume = {37},
  number = {12},
  pages = {1112--1118},
