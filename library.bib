@article{ocello2020,
  title = {Conduction and {{Gating Properties}} of the {{TRAAK Channel}} from {{Molecular Dynamics Simulations}} with {{Different Force Fields}}},
  author = {Ocello, Riccardo and Furini, Simone and Lugli, Francesca and Recanatini, Maurizio and Domene, Carmen and Masetti, Matteo},
