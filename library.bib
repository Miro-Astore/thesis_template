pages = {29--36},
pmid = {23891399},
publisher = {Elsevier},
title = {{Effect of ivacaftor on CFTR forms with missense mutations associated with defects in protein processing or function}},
url = {http://www.cysticfibrosisjournal.com/article/S1569199313001136/fulltext http://www.cysticfibrosisjournal.com/article/S1569199313001136/abstract https://www.cysticfibrosisjournal.com/article/S1569-1993(13)00113-6/abstract},
