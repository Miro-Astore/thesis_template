@article{mccarty2017,
  title = {A Variational Conformational Dynamics Approach to the Selection of Collective Variables in Metadynamics},
  author = {McCarty, James and Parrinello, Michele},
  year = {2017},
  month = nov,
  journal = {Journal of Chemical Physics},
  volume = {147},
  number = {20},
  pages = {204109},
  publisher = {{American Institute of Physics Inc.}},
  issn = {00219606},
  doi = {10.1063/1.4998598},
  abstract = {In this paper, we combine two powerful computational techniques, wellered metadynamics and time-lagged independent component analysis. The aim is to develop a new tool for studying rare events and exploring complex free energy landscapes. Metadynamics is a well-established and widely used enhanced sampling method whose efficiency depends on an appropriate choice of collective variables. Often the initial choice is not optimal leading to slow convergence. However by analyzing the dynamics generated in one such run with a time-lagged independent component analysis and the techniques recently developed in the area of conformational dynamics, we obtain much more efficient collective variables that are also better capable of illuminating the physics of the system. We demonstrate the power of this approach in two paradigmatic examples.},
  keywords = {free energy,independent component analysis,molecular configurations,molecular dynamics method,variational techniques},
  annotation = {\_eprint: 1703.08777}
}

