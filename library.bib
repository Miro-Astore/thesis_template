keywords = {Amyloid,Lysozyme,Myoglobin,Prediction,ROSETTADESIGN},
month = {mar},
number = {11},
pages = {4074--4078},
pmid = {16537487},
publisher = {National Academy of Sciences},
title = {{The 3D profile method for identifying fibril-forming segments of proteins}},
