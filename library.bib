@misc{sep2013,
  title = {Nominalism in the {{Philosophy}} of {{Mathematics}}},
  author = {Sep, Mon},
  year = {2013},
  pages = {1--39},
  doi = {10.1093/obo/9780195396577-0080},
  abstract = {Two views about mathematics: nominalism and platonism In ontological discussions about mathematics, two views are prominent. According to platonism, mathematical objects (as well as mathematical relations and structures) exist and are abstract; that is, they are not located in space and time and have no causal connection with us. Although this characterization of abstract objects is purely negative\textemdash indicating what such objects are not\textemdash in the context of mathematics it captures the crucial features the objects in questions are supposed to have. According to nominalism, mathematical objects (including, henceforth, mathematical relations and structures) do not exist, or at least they need not be taken to exist for us to make sense of mathematics. So, it is the nominalist's burden to show how to interpret mathematics without the commitment to the existence of mathematical objects. This is, in fact, a key feature of nominalism: those who defend the view need to show that it is possible to yield at least as much explanatory work as the platonist obtains, but invoking a meager ontology. To achieve that, nominalists in the philosophy of mathematics forge interconnections with metaphysics (whether mathematical objects do exist), epistemology (what kind of knowledge of these entities we have), and philosophy of science (how to make sense of the successful application of mathematics in science without being committed to the existence of mathematical entities). These interconnections are one of the sources of the variety of nominalist views.},
  file = {/home/miro/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Sep - 2013 - Nominalism in the Philosophy of Mathematics.html}
}

@article{serohijos2008,
  title = {Phenylalanine-508 Mediates a Cytoplasmic-Membrane Domain Contact in the {{CFTR 3D}} Structure Crucial to Assembly and Channel Function},
  author = {Serohijos, Adrian W.R. and Hegedus, Tam{\'a}s and Aleksandrov, Andrei A and He, Lihua and Cui, Liying and Dokholyan, Nikolay V and Riordan, John R},
  year = {2008},
  month = mar,
  journal = {Proceedings of the National Academy of Sciences of the United States of America},
  volume = {105},
  number = {9},
  pages = {3256--3261},
  publisher = {{National Academy of Sciences}},
  issn = {00278424},
  doi = {10.1073/pnas.0800254105},
  abstract = {Deletion of phenylalanine-508 (Phe-508) from the N-terminal nucleotide-binding domain (NBD1) of the cystic fibrosis transmembrane conductance regulator (CFTR), a member of the ATP-binding cassette (ABC) transporter family, disrupts both its folding and function and causes most cystic fibrosis. Most mutant nascent chains do not pass quality control in the ER, and those that do remain thermally unstable, only partially functional, and are rapidly endocytosed and degraded. Although the lack of the Phe-508 peptide backbone diminishes the NBD1 folding yield, the absence of the aromatic side chain is primarily responsible for defective CFTR assembly and channel gating. However, the site of interdomain contact by the side chain is unknown as is the high-resolution 3D structure of the complete protein. Here we present a 3D structure of CFTR, constructed by molecular modeling and supported biochemically, in which Phe-508 mediates a tertiary interaction between the surface of NBD1 and a cytoplasmic loop (CL4) in the C-terminal membrane-spanning domain (MSD2). This crucial cytoplasmic membrane interface, which is dynamically involved in regulation of channel gating, explains the known sensitivity of CFTR assembly to many disease-associated mutations in CL4 as well as NBD1 and provides a sharply focused target for small molecules to treat CF. In addition to identifying a key intramolecular site to be repaired therapeutically, our findings advance understanding of CFTR structure and function and provide a platform for focused biochemical studies of other features of this unique ABC ion channel. \textcopyright{} 2008 by The National Academy of Sciences of the USA.},
  pmid = {18305154},
  keywords = {ABC transporter,Cystic fibrosis,Domain interactions,Modeling,Protein misfolding}
}

