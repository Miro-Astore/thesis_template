@article{hutter2019,
  title = {The Extracellular Gate Shapes the Energy Profile of an {{ABC}} Exporter},
  author = {Hutter, Cedric A. J. and Timachi, M. Hadi and H{\"u}rlimann, Lea M. and Zimmermann, Iwan and Egloff, Pascal and G{\"o}ddeke, Hendrik and Kucher, Svetlana and {\v S}tefani{\'c}, Sa{\v s}a and Karttunen, Mikko and Sch{\"a}fer, Lars V. and Bordignon, Enrica and Seeger, Markus A.},
