@article{abraham2015,
  title = {Gromacs: {{High}} Performance Molecular Simulations through Multi-Level Parallelism from Laptops to Supercomputers},
  author = {Abraham, Mark James and Murtola, Teemu and Schulz, Roland and P{\'a}ll, Szil{\'a}rd and Smith, Jeremy C. and Hess, Berk and Lindah, Erik},
