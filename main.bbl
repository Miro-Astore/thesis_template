    \field{title}{Cystic Fibrosis Transmembrane Conductance Regulator
  ({{CFTR}}): {{Making}} an Ion Channel out of an Active Transporter Structure}
    \field{volume}{12}
    \field{journaltitle}{Channels}
    \field{month}{01}
    \field{year}{2018}
  \endentry

